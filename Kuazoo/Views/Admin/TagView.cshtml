@model com.kuazoo.Models.InventoryItemModel.Tag
@{
    ViewBag.Title = "Tag View";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script type="text/javascript">
    $(document).ready(function () {
        changeverticalnav(6);
    });
</script>
<div id="breadcrumbs">
    <div class="mainwrap">
        <a href="Tag">Tag</a> <span class="active"> / View Tag</span>
    </div>
</div>
<div id="main">
    <div id="mainwrap">
        <h1>View Tag</h1>
    </div>
    <div id="wrap">
        <div>
            <div class="colleft">@Html.LabelFor(model => model.TagName)</div>
            <div class="colright">@Model.TagName</div>
        </div>
        <div>
            <div class="colleft">@Html.LabelFor(model => model.ShowAsCategory)</div>
            <div class="colright">@Model.ShowAsCategory</div>
        </div>
        <div>
            <div class="colleft">@Html.LabelFor(model => model.ParentId)</div>
            <div class="colright">@Model.ParentName</div>
        </div> 
    </div>
            <div class="wrapend">
            <div class="colleft"></div>
            <div class="colright"><a href="TagEdit?TagId=@Model.TagId" class="submitBtn">Edit</a>
            <a href="Tag" class="grayBtn">Back</a></div>
        </div>
</div>