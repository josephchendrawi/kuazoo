@model com.kuazoo.Models.CountryModel
@{
    ViewBag.Title = "Country View";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script type="text/javascript">
    $(document).ready(function () {
        changeverticalnav(10);
    });
</script>
<div id="breadcrumbs">
    <div class="mainwrap">
        <a href="Country">Country</a> <span class="active"> / View Country</span>
    </div>
</div>
<div id="main">
    <div id="mainwrap">
        <h1>View Country</h1>
    </div>
    <div id="wrap">
        <div>
            <div class="colleft">@Html.LabelFor(model => model.CountryName)</div>
            <div class="colright">@Model.CountryName</div>
        </div>
    </div>
            <div class="wrapend">
            <div class="colleft"></div>
            <div class="colright"><a href="CountryEdit?CountryId=@Model.CountryId" class="submitBtn">Edit</a>
            <a href="Location#country" class="grayBtn">Back</a></div>
        </div>
</div>