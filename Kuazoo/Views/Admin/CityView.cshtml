@model com.kuazoo.Models.CityModel
@{
    ViewBag.Title = "City View";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script type="text/javascript">
    $(document).ready(function () {
        changeverticalnav(10);
    });
</script>
<div id="breadcrumbs">
    <div class="mainwrap">
        <a href="City">City</a> <span class="active"> / View City</span>
    </div>
</div>
<div id="main">
    <div id="mainwrap">
        <h1>View City</h1>
    </div>
    <div id="wrap">
        <div>
            <div class="colleft">@Html.LabelFor(model => model.CountryId)</div>
            <div class="colright">@Model.CountryName</div>
        </div>
        <div>
            <div class="colleft">@Html.LabelFor(model => model.CityName)</div>
            <div class="colright">@Model.CityName</div>
        </div>
    </div>
            <div class="wrapend">
            <div class="colleft"></div>
            <div class="colright"><a href="CityEdit?CityId=@Model.CityId" class="submitBtn">Edit</a>
            <a href="Location#city" class="grayBtn">Back</a></div>
        </div>
</div>